  "scripts": {
    "postinstall": "rm -rf bower_components && bower cache clean && bower install --force && bower prune && gulp build --release",
    "start": "node server.js",
    "test": "gulp test",
    "posttest": "npm run build-reports",
    "build-reports": "npm run remap-coverage && npm run coverage-html && npm run coverage-lcovonly",
    "remap-coverage": "cat build/reporters/coverage/coverage-bundle.json | ./node_modules/.bin/remap-istanbul -e node_modules > build/reporters/coverage/coverage.json",
    "coverage-html": "cat build/reporters/coverage/coverage-bundle.json | ./node_modules/.bin/remap-istanbul -e node_modules -t html -o build/reporters/coverage/html",
    "coverage-lcovonly": "cat build/reporters/coverage/coverage-bundle.json | ./node_modules/.bin/remap-istanbul -e node_modules -t lcovonly > build/reporters/coverage/lcov.info",
    "gulp": "./node_modules/.bin/gulp"
  }
